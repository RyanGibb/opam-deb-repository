  ["sh" "-c" "sudo dpkg -r libarpack++2c2a"]
]
depends: [
  "libarpack2" {>= "2.1"}
  "libc6" {>= "2.2.5"}
  "libgcc1" {>= "1-3.0"}
  "libstdc++6" {>= "4.4.0"}
  ("libblas3" | "libblas-so-3")
  ("liblapack3" | "liblapack-so-3")
]
conflicts: [
  "arpack++"
  "libarpack++2"
  "libarpack++2c2"
]
extra-source "libarpack++2c2a_2.3-9_amd64.deb" {

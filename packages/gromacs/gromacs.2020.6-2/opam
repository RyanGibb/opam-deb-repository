  "libgcc-s1" {>= "3.0"}
  "libgromacs5" {>= "2020.6"}
  "libstdc++6" {>= "5.2"}
  "libx11-6"
  "sse4-2-support"
]
extra-source "gromacs_2020.6-2_amd64.deb" {

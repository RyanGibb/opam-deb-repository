opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i libgtkgl2.0-dev_2.1.0-1_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r libgtkgl2.0-dev"]
]
depends: [
  "libgl-dev"
  "libgtk2-0-dev"
  "libgtkgl2-0-1" {= "2.1.0-1"}
]
extra-source "libgtkgl2.0-dev_2.1.0-1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/g/gtkgl2/libgtkgl2.0-dev_2.1.0-1_amd64.deb"
}

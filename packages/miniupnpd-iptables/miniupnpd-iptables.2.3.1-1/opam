build: [
  ["sh" "-c" "sudo dpkg -i miniupnpd-iptables_2.3.1-1_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r miniupnpd-iptables"]
]
depends: [
  "iptables"
  "libc6" {>= "2.34"}
  "libip4tc2" {>= "1.8.3"}
  "libip6tc2" {>= "1.8.3"}
]
conflicts: [
  "miniupnpd-nftables"
]
extra-source "miniupnpd-iptables_2.3.1-1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/m/miniupnpd/miniupnpd-iptables_2.3.1-1_amd64.deb"
}

opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i libgtkgl2.0-1_2.1.0-1_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r libgtkgl2.0-1"]
]
depends: [
  "libc6" {>= "2.4"}
  "libgl1"
  "libglib2-0-0" {>= "2.37.3"}
  "libgtk2-0-0" {>= "2.24.0"}
  "libx11-6"
]
extra-source "libgtkgl2.0-1_2.1.0-1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/g/gtkgl2/libgtkgl2.0-1_2.1.0-1_amd64.deb"
}

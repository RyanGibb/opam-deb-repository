  "python-any"
  "python-pyflakes" {= "2.0.0-1"}
]
extra-source "pyflakes_2.0.0-1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/p/pyflakes/pyflakes_2.0.0-1_all.deb"
}

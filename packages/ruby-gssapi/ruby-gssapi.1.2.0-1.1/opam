opam-version: "2.0"
build: [
  ["sh" "-c" "sudo dpkg -i ruby-gssapi_1.2.0-1.1_amd64.deb"]
]
remove: [
  ["sh" "-c" "sudo dpkg -r ruby-gssapi"]
]
depends: [
  "libgssapi-krb5-2"
  "ruby-ffi" {>= "1.0.1"}
  ("ruby" | "ruby-interpreter")
]
extra-source "ruby-gssapi_1.2.0-1.1_amd64.deb" {
  src: "http://ftp.debian.org/debian/pool/main/r/ruby-gssapi/ruby-gssapi_1.2.0-1.1_all.deb"
}
